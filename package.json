		"@react-navigation/native": "^7.1.8",
		"dotenv": "^17.3.1",
		"expo": "^54.0.33",
		"expo-blur": "~15.0.8",
		"expo-constants": "~18.0.13",
		"expo-crypto": "^15.0.8",
		"expo-device": "~8.0.10",
		"expo-file-system": "~19.0.21",
		"expo-glass-effect": "~0.1.9",
		"expo-haptics": "~15.0.8",
		"expo-linear-gradient": "~15.0.8",
		"expo-linking": "~8.0.11",
		"expo-router": "~6.0.23",
