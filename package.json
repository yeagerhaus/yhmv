		"fast-xml-parser": "^5.3.7",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.5",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-get-random-values": "^1.11.0",
		"react-native-image-colors": "^2.5.1",
		"react-native-reanimated": "~4.1.6",
		"react-native-safe-area-context": "~5.6.2",
